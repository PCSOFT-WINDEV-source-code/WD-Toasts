              code : |1+
               // Version 1
               // Description 
               // Combo box for selecting a system font
               
               
               sFontList	is string
               sFont		is string
               ListDeleteAll(MySelf)
               
               // List of system fonts
               sFontList	= FontList()
               
               sFont		= ExtractString(sFontList,firstRank,CR)
               while sFont<>EOT
               	if sFont<>"" then
               		// Add the font name into the Font style
               		ListAdd(MySelf,gFont(sFont)+sFont)
               	end	
               	sFont = ExtractString(sFontList,nextRank,CR)
               end
               
               // Set the position on the "Courier" font
               ListSelectPlus(MySelf,Max(ListSeek(MySelf,"Segoe UI"),1))
