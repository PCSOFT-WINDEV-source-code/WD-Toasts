           type_code : 4
           p_codes :
            -
              code : |1+
               
               t		is a Toast
               
               // Base elements
               nRow	is int
               for each string sRow of EDT_Message SEPARATED BY CR
               	t..Text += [CR] + gFontBold(nRow=1 _and_ CBOX_Style[1]) + ...
               	gFontItalic(nRow>1 _and_ CBOX_Style[2]) + ...
               	gFont(COMBO_Font[COMBO_Font]) + ...
               	gFontSize(COMBO_FontSize[COMBO_FontSize]) + ...
               	gPen(ColorContrast(EDT_Red,EDT_Green,EDT_Blue)) + sRow
               end
               
               t..DisplayDuration		= COMBO_Display_duration=1 ? toastShort else toastLong
               t..VerticalAlignment	= COMBO_Vertical_position-1
               t..HorizontalAlignment	= COMBO_Horizontal_position-1
               t..BackgroundColor		= RGB(EDT_Red,EDT_Green,EDT_Blue)
               
               // Additional properties of the Toast type
               t..CloseOnClick			= CBOX_ToastOptions[1]
               t..Overlayable			= CBOX_ToastOptions[2]
               t..Title				= EDT_Title
               t..Opacity				= SLD_Opacity
               
               	INTERNAL PROCEDURE ReturnClosureButton( local s is string ) : procedure
               		INTERNAL PROCEDURE ClickOnButton()
               		Info("You clicked on '[%s%]'")
               		end	
               	p is procedure = ClickOnButton
               	result p
               	end
               
               
               if COMBO_Buttons>1 then
               	sText1 is string = "More details..."
               	t..Button[1].Libellé		= gFont(COMBO_Font[COMBO_Font])+gPen(ColorContrast(EDT_Red,EDT_Green,EDT_Blue))+sText1
               	t..Button[1].ActionClic		= ReturnClosureButton(sText1)
               end
               if COMBO_Buttons>2 then
               	sText2 is string = "Do not show again"
               	t..Button[2].Libellé		= gFont(COMBO_Font[COMBO_Font])+gPen(ColorContrast(EDT_Red,EDT_Green,EDT_Blue))+sText2
               	t..Button[2].ActionClic		= ReturnClosureButton(sText2)
               end
               if COMBO_Buttons>3 then
               	sText3 is string = "Details..."
               	t..Button[3].Libellé		= gFont(COMBO_Font[COMBO_Font])+gPen(ColorContrast(EDT_Red,EDT_Green,EDT_Blue))+sText3
               	t..Button[3].ActionClic		= ReturnClosureButton(sText3)
               end
               
               t..Button..HorizontalAlignment = COMBO_Horizontal_alignment_of_buttons-1
               
               ToastDisplay(t)
