     procedure_id : 2216427347801905910
     type_code : 14
     code : |1+
      procedure ColorContrast( local r is int, local v is int, local b is int )
      
      if ( 0.299 * r + 0.587 * v + 0.114 * b)/255 > 0.5 then
      	result Black
      else
      	result White
      end
