              internal_properties : CAAAAAgAAABow7SSjyw4+RaG603Ketue25DcXFr/Ad4cxtglbfhq10IPQeS5HrR9fDTW0PYi5UrgEIxl/pLZc0yIqc60sDT5I5w6ekQWk6rakv66ZN32ZfbbMd2V3oso5In0dqtU7o8b2vlbrCHtluhTTm00yMEgvZsX2vClYAfwrWz+faFPhs4AIurHJm1IPGlr8TRUte0LUuWYiwA=
              procedure_id : 2216427347801971446
              type_code : 14
              code : |1-
               procedure UpdColor(local dColor is a Color = White)
               
               // Update color preview
               STC_Color..BackgroundColor	= dColor
               
               // Update hexa HTML value of the color without #
               EDT_HTML					= Upper(RGBToHTML(dColor)[[2 to]])
               
               // Update input (only if different, so as not to move the user's cursor)
               if not EDT_Red = dColor..Red	then EDT_Red  = dColor..Red
               if not EDT_Green  = dColor..Green 	then EDT_Green   = dColor..Green
               if not EDT_Blue  = dColor..Blue 	then EDT_Blue   = dColor..Blue
              type : 458752
            -
              name : UpdateFromRGB
              procedure_id : 2216427347802036982
              type_code : 14
              code : |1+
               procedure UpdateFromRGB()
               
               // Convert RGB value to color
               d is a Color = RGB( Val(EDT_Red) ,Val(EDT_Green), Val(EDT_Blue) )
               
               // Update UI with this color
               UpdColor(d)
