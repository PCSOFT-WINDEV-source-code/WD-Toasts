         -
           code : |1+
            // Version 1
            // Description 
            // Display a clock
            // Time automatically updated
            // Assign time in WLanguage with DateTime, Local time or another time zone
            
            // Drawing parameters
            nDepartment					is int	= IMG_Drawing..Width / 2
            
            // Needle colors
            nColorSecond				is int	= RGB(220, 56, 43)
            nColorMinute				is int	= RGB(55, 70, 73)
            nColorHour					is int	= RGB(55, 70, 73)
            nTurningWhite				is int	= 20
            
            // Needle size and thickness
            nSizeHour					is int	= 0.70 * nDepartment - nTurningWhite
            nSizeMinute					is int	= 0.86 * nDepartment - nTurningWhite
            nSizeSecond					is int	= 0.96 * nDepartment - nTurningWhite
            nThicknessHour				is int	= 5
            nThicknessMinute			is int	= 3
            nThicknessSecond			is int	= 1
            
            	// Draw the clock
            	INTERNAL PROCEDURE DrawClock
            	
            	// Initialization of drawing
            	dStartDrawing(IMG_Drawing,dErase)
            	dChangeMode(IMG_Drawing,drawAntiAliasing)	
            	
            	// Time to draw
            	tLocalTime is time = TimeSys()
            	IMG_Drawing..ToolTip	= TimeToString(tLocalTime,"HH:MM:SS")
            	STC_Hour				= IMG_Drawing..ToolTip	
            	
            	// Hour hand
            	nNeedleAngle is int = ((( (tLocalTime..Hour modulo 12) * 60) + (tLocalTime..Minute)) / 2) - 90
            	dLine(IMG_Drawing,nDepartment + (Cos(nNeedleAngle) * nSizeHour), nDepartment + (Sin(nNeedleAngle) * nSizeHour), nDepartment, nDepartment, nColorHour, nThicknessHour)
            	
            	// Minute hand
            	nNeedleAngle = (((tLocalTime..Minute * 60) + (tLocalTime..Second)) / 10) - 90
            	dLine(IMG_Drawing,nDepartment + (Cos(nNeedleAngle) * nSizeMinute), nDepartment + (Sin(nNeedleAngle) * nSizeMinute), nDepartment, nDepartment, nColorMinute, nThicknessMinute)
            	
            	// Second hand
            	nNeedleAngle = (tLocalTime..Second * 6) - 90
            	dLine(IMG_Drawing,nDepartment + (Cos(nNeedleAngle) * nSizeSecond), nDepartment + (Sin(nNeedleAngle) * nSizeSecond),nDepartment, nDepartment, nColorSecond, nThicknessSecond)
            	
            	end
            
            // Draw the clock for the first time
            DrawClock()
            
            // Re-draws the clock every second
            TimerSys(DrawClock,1s)
           type : 14
