           code : |1+
            // Select time zone among those defined by the IANA Time Zone Database (to change according to preference)
            // e.g.:
            // Africa/Abidjan
            // Africa/Accra
            // Africa/Addis_Ababa
            // Africa/Algiers
            // Africa/Asmara
            // Africa/Bamako
            // etc
            STC_Different_time_zone = <ยง$0000ยง>
            
            // Drawing parameters
            nDepartment					is int	= IMG_DrawTimeZone..Width / 2
            
            // Needle colors
            nColorSecond				is int	= RGB(220, 56, 43)
            nColorMinute				is int	= RGB(55, 70, 73)
            nColorHour					is int	= RGB(55, 70, 73)
            nTurningWhite				is int	= 20
            
            // Needle size and thickness
            nSizeHour					is int	= 0.70 * nDepartment - nTurningWhite
            nSizeMinute					is int	= 0.86 * nDepartment - nTurningWhite
            nSizeSecond					is int	= 0.96 * nDepartment - nTurningWhite
            nThicknessHour				is int	= 5
            nThicknessMinute			is int	= 3
            nThicknessSecond			is int	= 1
            
            	// Draw the clock
            	INTERNAL PROCEDURE DrawClock
            	
            	// Initialization of drawing
            	dStartDrawing(IMG_DrawTimeZone,dErase)
            	dChangeMode(IMG_DrawTimeZone,drawAntiAliasing)	
            	
            	// Time to draw
            	dtDateTimeTimeZone	is datetime	= DateTimeLocalToTimeZone(SysDateTime(),STC_Different_time_zone)
            	tTimeTimeZone		is time		= dtDateTimeTimeZone..Time
            	IMG_DrawTimeZone..ToolTip	= TimeToString(tTimeTimeZone,"HH:MM:SS")
            	STC_TimeTimeZone			= IMG_DrawTimeZone..ToolTip	
            	
            	// Hour hand
            	nNeedleAngle is int = ((( (tTimeTimeZone..Hour modulo 12) * 60) + (tTimeTimeZone..Minute)) / 2) - 90
            	dLine(IMG_DrawTimeZone,nDepartment + (Cos(nNeedleAngle) * nSizeHour), nDepartment + (Sin(nNeedleAngle) * nSizeHour), nDepartment, nDepartment, nColorHour, nThicknessHour)
            	
            	// Minute hand
            	nNeedleAngle = (((tTimeTimeZone..Minute * 60) + (tTimeTimeZone..Second)) / 10) - 90
            	dLine(IMG_DrawTimeZone,nDepartment + (Cos(nNeedleAngle) * nSizeMinute), nDepartment + (Sin(nNeedleAngle) * nSizeMinute), nDepartment, nDepartment, nColorMinute, nThicknessMinute)
            	
            	// Second hand
            	nNeedleAngle = (tTimeTimeZone..Second * 6) - 90
            	dLine(IMG_DrawTimeZone,nDepartment + (Cos(nNeedleAngle) * nSizeSecond), nDepartment + (Sin(nNeedleAngle) * nSizeSecond),nDepartment, nDepartment, nColorSecond, nThicknessSecond)
            	
            	end
            
            // Draw the clock for the first time
            DrawClock()
            
            // Re-draws the clock every second
            TimerSys(DrawClock,1s)
            
